{
  "name": "luma-docs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate:versions": "node src/tools/generate-versions.js",
    "generate:routes": "node src/tools/generate-routes.js",
    "generate:search-index": "node src/tools/generate-search-index.js",
    "generate:sitemap": "node src/tools/generate-sitemap.js",
    "validate:frontmatter": "node src/tools/validate-frontmatter.mjs",
    "build:css": "tailwindcss -i ./src/styles/input.css -o ./src/public/index.css",
    "build:css:watch": "tailwindcss -i ./src/styles/input.css -o ./src/public/index.css --watch",
    "pipeline:dev": "node src/tools/run-build-pipeline.mjs dev",
    "pipeline:build": "node src/tools/run-build-pipeline.mjs build",
    "dev": "npm run pipeline:dev && vite-react-ssg dev",
    "dev:subfolder": "VITE_FORCE_BASE=/luma-docs/ npm run dev",
    "build": "npm run pipeline:build && vite-react-ssg build",
    "snapshot:version": "node src/tools/snapshot-version.js",
    "search-dev": "node src/tools/search-dev.js",
    "search:generate": "node src/tools/search-dev.js generate",
    "search:validate": "node src/tools/search-dev.js validate",
    "clean": "rm -rf dist/ node_modules/.vite/",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mdx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.mdx --fix",
    "type-check": "npm run pipeline:dev && tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/prismjs": "^1.26.5",
    "gray-matter": "^4.0.3",
    "prismjs": "^1.30.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "jest-axe": "^10.0.0",
    "zod": "^4.1.11",
    "@eslint/js": "^9.36.0",
    "@eslint/markdown": "^7.3.0",
    "@mdx-js/rollup": "^3.0.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.14.0",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vite-react-ssg": "^0.8.9",
    "vitest": "^3.2.4",
    "@vitejs/plugin-react": "^5.0.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/dom": "^10.4.1",
    "jsdom": "^24.1.3"
  }
}
